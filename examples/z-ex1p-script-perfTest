mpirun -np 4 ./ex1p -r 3 -o 4 -ser > junk.txt
mpirun -np 4 ./ex1p -r 4 -o 2 -ser > junk.txt
mpirun -np 4 ./ex1p -r 4 -o 3 -ser > junk.txt
